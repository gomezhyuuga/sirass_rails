<table class="table table-bordered">
	<thead>
		<tr>
			<th>Ver</th>
			<% if can? :manage, MonthlyReport %>
			<th>Prestador</th>
			<% end %>
			<th>De</th>
			<th>A</th>
			<th>Horas</th>
			<th>Estado</th>
			<th>Acci√≥n</th>
		</tr>
	</thead>
	<tbody>
	<% reportes.each do |r| %>
		<tr>
			<td class="valign center">
				<%= link_to control_hora_path(r) do %>
				<i class="icon-eye-open"></i>
				<% end %>
			</td>
			<% if can? :manage, MonthlyReport %>
			<td class="valign">
				<%= link_to r.inscripcion.prestador.nombre_completo, r.inscripcion.prestador %>
			</td>
			<% end %>
			<td class="valign">
				<%= r.fecha_inicio %>
			</td>
			<td class="valign">
				<%= r.fecha_fin %>
			</td>
			<td class="valign">
				<%= r.horas %>
			</td>
			<td class="center valign">
				<span class="label <%= lbl_estado_reporte(r) %>">
					<%= r.estado_reporte.descripcion %>
				</span>
			</td>
			<td class="valign">
				<%= render 'monthly_reports/admin_btn', obj: r %>
			</td>
		</tr>
	<% end %>
	</tbody>
</table>