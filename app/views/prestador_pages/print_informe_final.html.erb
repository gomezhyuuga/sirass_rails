<h4>Objetivos Específicos</h4>
<%= simple_format h( @reporte.obj_esp ) %>

<h4>Metodología</h4>
<%= simple_format h( @reporte.metodologia ) %>

<h4>Actividades realizadas</h4>
<%= simple_format h( @reporte.actividades ) %>

<h4>Metas</h4>
