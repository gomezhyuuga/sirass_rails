<% provide :title, "INFORME FINAL DE ACTIVIDADES" %>

<div class="row">
	<div class="span2">
		<p>
			<strong>Inscripción:</strong> #<%= @reporte.inscripcion.id %>
		</p>
	</div>
	<div class="span5 offset1">
		<p>
			<strong>Prestador:</strong> <%= @reporte.inscripcion.prestador.nombre_completo %>
		</p>
	</div>
</div>
<p>
	<strong>Estado:</strong>
	<span class="label <%= lbl_estado_reporte(@reporte) %>">
		<%= @reporte.estado_reporte.descripcion %>
	</span>
</p>
<hr>
<p>
	<strong>Horas:</strong> <%= @reporte.horas %>
</p>

<h4>Introducción</h4>
<%= simple_format h( @reporte.introduccion ) %>

