<% if inscripcion %>
	<% case inscripcion.estado_inscripcion_id when false %>
		<%= render 'prestador_pages/messages/no_inscrito' %>
		<p><%= current_user.prestador.articulo_id %></p>
	<% when EstadoInscripcion::VALIDANDO %>
		<%= render 'prestador_pages/messages/validando', inscripcion: inscripcion %>
	<% when EstadoInscripcion::EN_SERVICIO %>
		<%= render '/prestador_pages/messages/ya_inscrito', inscripcion: inscripcion %>
	<% when EstadoInscripcion::ERRORES %>
		<%= render '/prestador_pages/messages/con_errores', inscripcion: inscripcion %>
	<% when EstadoInscripcion::CORRECTA %>
		<%= render '/prestador_pages/messages/correcta', inscripcion: inscripcion %>
	<% end %>
<% else %>
	<% if articulo91 %>
		<%= render 'prestador_pages/messages/articulo' %>
	<% else %>
		<%= render 'prestador_pages/messages/no_inscrito' %>
	<% end %>
<% end %>